{
  "extends": ["airbnb-typescript-prettier"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./src"]],
        "extensions": [".ts", ".js", ".json"]
      }
    }
  }
}
